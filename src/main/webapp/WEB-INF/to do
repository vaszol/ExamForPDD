<%=request.getAttribute("test.otvetText1")%>
        			<% for(int i=1; i<7; i++){ %>
        			<% if(request.getAttribute(Integer.toString(i))!=""){ %>
        			<p><input type="radio" name="variant" value="<%=i%>" id="labeled_<%=i%>"/> 
        			<label for="labeled_<%=i%>"><%=request.getAttribute(Integer.toString(i)) %> </label></p>
					<% } %>
					<% } %>
					
	
