<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">
    <display-name>Students personnel2</display-name>
    <listener>
        <listener-class>ru.vaszol.exam.web.SessionManager</listener-class>
    </listener>
    <context-param>
        <param-name>javax.servlet.jsp.jstl.fmt.request.charset</param-name>
        <param-value>UTF-8</param-value>
    </context-param>
    <servlet>
        <servlet-name>MainFrameServlet</servlet-name>
        <servlet-class>ru.vaszol.exam.web.MainFrameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentFrameServlet</servlet-name>
        <servlet-class>ru.vaszol.exam.web.StudentFrameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SimpleList</servlet-name>
        <servlet-class>ru.vaszol.exam.web.SimpleList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BiletFrame</servlet-name>
        <servlet-class>ru.vaszol.exam.web.BiletFrameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VoprosEdit</servlet-name>
        <servlet-class>ru.vaszol.exam.web.VoprosEditFrameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestPDD</servlet-name>
        <servlet-class>ru.vaszol.exam.web.TestPDDServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>startExam</servlet-name>
        <servlet-class>ru.vaszol.exam.web.startExamServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestOver</servlet-name>
        <servlet-class>ru.vaszol.exam.web.TestOverServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>resultTest</servlet-name>
        <servlet-class>ru.vaszol.exam.web.resultTestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowVoproses</servlet-name>
        <servlet-class>ru.vaszol.exam.web.ShowVoproses</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>ru.vaszol.exam.web.AdminServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainFrameServlet</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentFrameServlet</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SimpleList</servlet-name>
        <url-pattern>/simple</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BiletFrame</servlet-name>
        <url-pattern>/bilets</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VoprosEdit</servlet-name>
        <url-pattern>/vopros</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestPDD</servlet-name>
        <url-pattern>/TestPDD</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>startExam</servlet-name>
        <url-pattern>/startExam</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestOver</servlet-name>
        <url-pattern>/TestOver</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>resultTest</servlet-name>
        <url-pattern>/resultTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowVoproses</servlet-name>
        <url-pattern>/ShowVoproses</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/AdminServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>startExam.jsp</welcome-file>
    </welcome-file-list>
    <resource-ref>
        <description>DB Connection</description>
        <res-ref-name>jdbc/StudentsDS</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>
    <servlet>
        <description></description>
        <display-name>managerTest</display-name>
        <servlet-name>managerTest</servlet-name>
        <servlet-class>ru.vaszol.exam.web.ManagerTestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>managerTest</servlet-name>
        <url-pattern>/managerTest</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>showResult</display-name>
        <servlet-name>showResult</servlet-name>
        <servlet-class>ru.vaszol.exam.web.showResult</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>showResult</servlet-name>
        <url-pattern>/showResult</url-pattern>
    </servlet-mapping>
</web-app>
